<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="cron_check_municipalities_distritopostal" forcecreate="True" model="ir.cron">
        <field name="name">Cron Check Municipalities (Distritopostal)</field>
        <field name="active" eval="False"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_distritopostal_municipality"/>
        <field name="state">code</field>
        <field name="code">model.cron_check_municipalities_distritopostal()</field>
    </record>

    <record id="cron_check_ways_distritopostal" forcecreate="True" model="ir.cron">
        <field name="name">Cron Check Ways (Distritopostal)</field>
        <field name="active" eval="False"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_distritopostal_way"/>
        <field name="state">code</field>
        <field name="code">model.cron_check_ways_distritopostal()</field>
    </record>

</odoo>
